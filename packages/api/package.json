{
  "name": "@minatokens/api",
  "description": "MinaTokens API",
  "version": "0.9.4",
  "author": "DFST",
  "bugs": "https://github.com/zkcloudworker/minatokens-lib/issues",
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.60.0",
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.0",
    "typedoc": "^0.27.3",
    "typedoc-plugin-markdown": "^4.3.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    "types": "./dist/node/index.d.ts",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "browser": "./dist/web/index.js",
    "default": "./dist/web/index.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://docs.minatokens.com",
  "keywords": [
    "cloud",
    "fungible-token",
    "mina-protocol",
    "minatokens",
    "NFT",
    "proving",
    "tokens",
    "typescript",
    "zkcloudworker"
  ],
  "license": "Apache-2.0",
  "main": "dist/web/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "zkcloudworker/minatokens-lib.git",
  "scripts": {
    "build": "rm -rf dist/lib && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "docs": "typedoc",
    "login": "npm login",
    "release": "npm run build && npm publish --access public",
    "validate": "swagger-cli validate open-api.yaml",
    "generate": "npm run validate && openapi-ts && rm src/client/index.ts && node replace-import.cjs && npm run build"
  },
  "type": "module",
  "types": "./dist/node/index.d.ts",
  "dependencies": {
    "@hey-api/client-fetch": "^0.5.6"
  }
}
