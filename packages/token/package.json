{
  "name": "@minatokens/token",
  "version": "0.5.0",
  "author": "DFST",
  "description": "MinaTokens Fungible Token Library",
  "type": "module",
  "keywords": [
    "typescript",
    "mina-protocol",
    "zkcloudworker",
    "minatokens",
    "tokens",
    "fungible-token",
    "cloud",
    "proving"
  ],
  "main": "dist/web/index.js",
  "exports": {
    "types": "./dist/node/index.d.ts",
    "browser": "./dist/web/index.js",
    "node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "default": "./dist/web/index.js"
  },
  "types": "./dist/node/index.d.ts",
  "homepage": "https://docs.minatokens.com",
  "bugs": {
    "url": "https://github.com/zkcloudworker/minatokens-lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkcloudworker/minatokens-lib.git"
  },
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "rm -rf dist/lib && tsc && tsc --build tsconfig.web.json && node build-cjs.js",
    "release": "npm run build && npm publish --access public --otp",
    "login": "npm login",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@minatokens/api": "^0.5.0",
    "@minatokens/storage": "^0.5.0",
    "@types/node": "^22.10.0",
    "esbuild": "^0.24.0",
    "o1js": "^2.1.0",
    "typedoc": "^0.27.3",
    "typedoc-plugin-markdown": "^4.3.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@minatokens/api": "^0.5.0",
    "@minatokens/storage": "^0.5.0",
    "o1js": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
